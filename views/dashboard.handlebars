<div class="row text-center">
    <div class="col-auto">
        <h2>Welcome, {{userDashboard.[0].user.user_name}}!</h2>
    </div>
</div>

<div class="text-center">
    <div class="col-md-12">
        <h4>
            <a href="/dashboard/new">Create a New Article</a>
        </h4>
    </div>
</div>
<div class="col-md-12">
    <h3>Current Articles:</h3>

    {{#each userDashboard}}
        <div class="row">
            <div class="col-md-8">
                <h5><a href="/articles/{{this.id}}">{{this.title}}</a></h5>
            </div>
            <div class="col-md-4">
                <a href="/dashboard/edit/{{this.id}}" type="button" class="btn btn-warning">Edit</a>
            </div>
        </div>
    {{/each}}
</div>


<script src="./js/.js"></script>